{
    "name": "create-frontend-template",
    "version": "1.0.0",
    "scripts": {
        "cp:utils": "npx tsx scripts/copy/utils/copy-utils.ts",
        "build:upgrade-mac": "npx @vercel/ncc build ./scripts/upgrade/change-upgrade-version-mac.ts -o ./dist/upgrade-mac && tsx scripts/write/upgrade-write-mac.ts",
        "build:upgrade-win": "npx @vercel/ncc build ./scripts/upgrade/change-upgrade-version-win.ts -o ./dist/upgrade-win && tsx scripts/write/upgrade-write-win.ts",
        "cp:template": "cd scripts/copy/template && npx tsx copy-template-main.ts",
        "cp:win-upgrade": "tsx scripts/copy/upgrade/copy-build-upgrade-win.ts",
        "cp:mac-upgrade": "tsx scripts/copy/upgrade/copy-build-upgrade-mac.ts",
        "build:tmp": "npx @vercel/ncc build ./packages/create-template/src/index.ts -o ./dist/react-tmp && cd scripts/write && tsx tmp-write.ts",
        "cp:cli-mac": "tsx scripts/copy/build/copy-build-tmp-mac.ts",
        "cp:cli-win": "tsx scripts/copy/build/copy-build-tmp-win.ts",
        "cp:keyboard": "cd scripts/copy/test && npx tsx copy-keyboard.ts",
        "generate-csstype": "tsx ./scripts/css/generate-css-type.ts",
        "create:env": "cd scripts/create-env && npx tsx create-env.ts",
        "test:react-dev": "cd test/react/test-dev && uv venv && uv pip install -r requirements.txt && uv run main.py",
        "test:react-pack": "pnpm build:tmp && pnpm cp:cli-mac && pnpm cp:cli-win && cd test/react/test-pack && uv venv && uv pip install -r requirements.txt && uv run main.py",
        "test:vue-dev": "cd test/vue/test-dev && uv venv && uv pip install -r requirements.txt && uv run main.py",
        "test:vue-pack": "pnpm build:tmp && pnpm cp:cli-mac && pnpm cp:cli-win && cd test/vue/test-pack && uv venv && uv pip install -r requirements.txt && uv run main.py",
        "test:dev-all": "pnpm test:react-dev && pnpm test:vue-dev",
        "test:pack-all": "pnpm test:react-pack && pnpm test:vue-pack",
        "test:all": "pnpm test:dev-all && pnpm test:pack-all",
        "lib:storybook": "cd packages/lib && pnpm storybook",
        "lib:test": "cd packages/lib && pnpm test"
    },
    "dependencies": {
        "@commitlint/cli": "^19.8.1",
        "@commitlint/config-conventional": "^19.8.1",
        "@types/adm-zip": "^0.5.7",
        "adm-zip": "^0.5.16",
        "conf": "^15.0.2",
        "csstype": "^3.2.3",
        "node-pty": "^1.1.0",
        "picocolors": "^1.1.1",
        "prettier": "^3.6.2"
    },
    "devDependencies": {
        "@types/conf": "^3.0.3",
        "@types/node": "^25.0.1",
        "@vitejs/plugin-react": "^5.0.4",
        "assert": "^2.1.0",
        "browserify-zlib": "^0.2.0",
        "buffer": "^6.0.3",
        "copy-webpack-plugin": "^13.0.1",
        "process": "^0.11.10",
        "stream-browserify": "^3.0.0",
        "ts-loader": "^9.5.4",
        "tsx": "^4.20.3",
        "typescript": "^5.8.3"
    }
}
